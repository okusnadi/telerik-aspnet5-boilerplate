{
  "name": "TelerikBoilerplate",
  "private": true,
  "dependencies": {
    "bootstrap": "3.0.0",
    "jquery": "2.1.4",
    "jquery-validation": "1.11.1",
    "jquery-validation-unobtrusive": "3.2.2"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/*.**",
        "dist/css/*.**",
        "dist/fonts/*.**"
      ]
    },
    "jquery": {
      "main": "dist/jquery.{js,min.js,min.map}"
    },
    "jquery-validation": {
      "main": "jquery.validate.js"
    },
    "jquery-validation-unobtrusive": {
      "main": "jquery.validate.unobtrusive{.js,.min.js}"
    }
  }
}
